<rotate
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90">

    <shape android:shape="line">
        <size android:width="32dp" android:height="32dp" />
        <stroke
            android:color="#AAAAAA"
            android:dashGap="2dp"
            android:dashWidth="4dp"
            android:width="1dp"/>
    </shape>
</rotate>